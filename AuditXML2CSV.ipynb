    "import csv\n",
    "import xml.etree.ElementTree as ET\n",
    "\n",
    "def flattenXML(xmlstring, auditbatch):\n",
    "    tree = ET.ElementTree(ET.fromstring(xmlstring))\n",
    "    root = tree.getroot()\n",
    "    csvlist=[]\n",
    "    #auditbatch=0\n",
    "    #auditline=0\n",
    "    idline=0\n",
    "    attrline=0\n",
    "    csvline=\"\"\n",
    "    action=\"\"\n",
    "    audittype=\"\"\n",
    "    idtype=\"\"\n",
    "    idname=\"\"\n",
    "    idvalue=\"\"\n",
    "    attrtype=\"\"\n",
    "    attrname=\"\"\n",
    "    newvalue=\"\"\n",
    "    oldvalue=\"\"\n",
    "    if root.tag == \"OrderAuditDetails\":\n",
    "        #auditbatch += 1\n",
    "        auditline=0\n",
    "        csvline = \"\"\n",
    "        shortline=\"\"\n",
    "        for layer1 in root.findall(\"*\"):\n",
    "            if layer1.tag == \"OrderAuditDetail\":\n",
    "                if 'Action' in layer1.attrib:\n",
    "                    action = str(layer1.attrib['Action'])\n",
    "                if 'AuditType' in layer1.attrib:\n",
    "                    audittype = str(layer1.attrib['AuditType'])\n",
    "                auditline += 1\n",
    "                csvline = str(auditbatch) + \",\" + str(auditline) + \",\" + action + \",\" + audittype\n",
    "               # print(layer1.tag)\n",
    "                for layer2 in layer1.findall(\"*\"): #get ids and attributes\n",
    "                    #print(layer2.tag)\n",
    "                    if layer2.tag == \"IDs\":\n",
    "                        for layer3 in layer2.findall(\"*\"):\n",
    "                            if layer3.tag == \"ID\":\n",
    "                                if 'DataType' in layer3.attrib:\n",
    "                                    idtype = str(layer3.attrib['DataType'])\n",
    "                                if 'Name' in layer3.attrib:\n",
    "                                    idname = str(layer3.attrib['Name'])\n",
    "                                if 'Value' in layer3.attrib:\n",
    "                                    idvalue = str(layer3.attrib['Value'])\n",
    "                                idline += 1\n",
    "                                csvline = csvline + \",\" + str(idline) + \",\" + idtype + \",\" + idname + \",\" + idvalue\n",
    "                        if idline < 6:\n",
    "                            while idline < 6: #matrix of 8 ID columns\n",
    "                                idline += 1\n",
    "                                csvline = csvline + \",\" + str(idline) + \",,,\"\n",
    "                    if layer2.tag == \"Attributes\":\n",
    "                        for layer3 in layer2.findall(\"*\"):\n",
    "                            if layer3.tag == \"Attribute\":\n",
    "                                if 'DataType' in layer3.attrib:\n",
    "                                    attrtype = str(layer3.attrib['DataType'])\n",
    "                                if 'Name' in layer3.attrib:\n",
    "                                    attrname = str(layer3.attrib['Name'])\n",
    "                                if 'NewValue' in layer3.attrib:\n",
    "                                    newvalue = str(layer3.attrib['NewValue'])\n",
    "                                if 'OldValue' in layer3.attrib:\n",
    "                                    oldvalue = str(layer3.attrib['OldValue'])\n",
    "                                attrline += 1\n",
    "                                csvline = csvline + \",\" + str(attrline) + \",\" + attrtype + \",\" + attrname + \",\" + newvalue + \",\" + oldvalue\n",
    "                        if attrline < 15:\n",
    "                            while attrline < 15: #matrix of 20 attributes per audit line\n",
    "                                attrline += 1\n",
    "                                csvline = csvline + \",\" + str(attrline) + \",,,,\"                                            \n",
    "                csvlist.append(csvline)\n",
    "                csvline=\"\"\n",
    "                attrline=0\n",
    "                idline=0\n",
    "    return csvlist\n",
    "                            \n",
    "\n",
	"# REPLACE ##MY with relevant file names.  May require different loop nesting based on how audit records are organized"
    "with open(\"##MY_order_audit.del\", \"r\") as fa:\n",
    "    with open(\"##MY_order_audit_level.del\", \"r\") as fb:\n",
    "        with open(\"##MY_order_audit_detail.del\", \"r\") as fc:\n",
    "            with open(\"##MY_order_audit_detail.del.001.xml\", \"r\") as fd:\n",
    "                with open(\"audit_flat02.csv\", \"w\") as ff:\n",
    "                    listsofa = [line.split(\",\") for line in fa]\n",
    "                    listsofb = [line.split(\",\") for line in fb]\n",
    "                    listsofc = [line.split(\",\") for line in fc]\n",
    "                    #listsofd = [line.split() for line in fd]\n",
    "                    #lista[x][0] is the audit_key --> listb[x][1]; #listb[x][1] & [x][0] --> listc[x][1] and [x][2]\n",
    "                    for linea in listsofa:\n",
    "                        for lineb in listsofb:\n",
    "                            if linea[0].strip() == lineb[1].strip():\n",
    "                                for linec in listsofc:\n",
    "                                    if linec[1].strip() == lineb[1].strip() and linec[2].strip() == lineb[0].strip():\n",
    "                                        outline=\"\"\n",
    "                                        for part in linea:\n",
    "                                            outline += part.strip() + \",\"\n",
    "                                        for part in lineb:\n",
    "                                            outline += part.strip() + \",\"\n",
    "                                        #auditbatch=0\n",
    "                                        lineclist=[]\n",
    "                                        linecline=\"\"\n",
    "                                        for part in linec:\n",
    "                                            if \"<XDS FIL=\" in part:\n",
    "                                                xds = part.split(\"'\")\n",
    "                                                offset=int(xds[3])\n",
    "                                                num=int(xds[5])\n",
    "                                                #print(part + \"offset:\" + str(offset) + \"len:\" + str(num))\n",
    "                                                fd.seek(offset)\n",
    "                                                xmlstring = fd.read(num)\n",
    "                                                auditbatch += 1\n",
    "                                                lineclist = flattenXML(xmlstring, auditbatch)\n",
    "                                            else:\n",
    "                                                linecline += part.strip() + \",\"\n",
    "                                        tmpoutline=outline\n",
    "                                        if int(lineclist[7]) == 0:\n",
    "                                            shortline=\n",
    "                                        for csvline in lineclist:\n",
    "                                            #print(outline + linecline + csvline)\n",
    "                                            ff.write(outline + linecline + csvline + \"\\n\")\n",
    "                                    \n",
    "                                    #print(\"Header:\" + str(linea)+\", Level:\"+str(lineb)+\", Detail:\"+str(linec))\n",
    "                        #if listb[1] == lista[0]:\n",
    "                            #print(str(lista)+\",\"+str(listb))\n",
    "    #reader = csv.reader(f, delimiter=\"\\t\")\n",
    "     "
